# AI API Keys (choose one or both)
# Get OpenAI API key from: https://platform.openai.com/api-keys
VITE_OPENAI_API_KEY=your_openai_api_key_here

# Get Anthropic API key from: https://console.anthropic.com/
VITE_ANTHROPIC_API_KEY=your_anthropic_api_key_here

# Optional: Custom API base URL for development
# Leave empty for production (auto-detects Vercel environment)
VITE_API_BASE_URL=http://localhost:3001

# Cloudinary Configuration (for image hosting)
# Get your credentials from: https://cloudinary.com/console
CLOUDINARY_CLOUD_NAME=your_cloud_name_here
CLOUDINARY_API_KEY=your_api_key_here
CLOUDINARY_API_SECRET=your_api_secret_here

# eBay Developer Credentials (for API integration)
# Get credentials from: https://developer.ebay.com/
EBAY_CLIENT_ID=your_ebay_client_id_here
EBAY_CLIENT_SECRET=your_ebay_client_secret_here
EBAY_SANDBOX=true # Set to false for production